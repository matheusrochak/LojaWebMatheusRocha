{
  "compilerOptions": {
   
    "target": "ES6",                                
    "module": "commonjs",                                
    "rootDir": "./src",                                  
    "resolveJsonModule": true,                       

    "outDir": "./dist",                                    
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                     
    "skipLibCheck": true 

  } , 
  "exclude": [

    "node_extension" ]
}
  

